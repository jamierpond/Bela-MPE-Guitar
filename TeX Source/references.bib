@article{john_personalisation_2020,
	title = {Personalisation of {Augmented} {Guitar}: {A} {Machine} {Learning} {Approach}},
	abstract = {This paper is trying to bring out a fully personalizable guitar with sonic possibilities much higher than the traditional guitars. The augmentation of guitar is done by adding a customisable guitar MIDI (Musical Instrument Digital Interface) controller to the guitar body. Different parameters of the guitar effects can be changed by different hand gestures. This will provide a guitarist multiple dimensions of control for different effects parameters. Other than that, machine learning approach is used to recognise the artist who is playing the guitar. This would help to provide an even more personalised experience. A user study is conducted to find the 2-dimensional position of the guitar while each artist is playing the instrument. This method is only applicable to closed data set. So, for example, this can be implemented for all the members of a band. This technique can even be used for giving passwords to individual instruments; which then turn on or open a special effect patch only when the corresponding artist pick up the instrument. This research paper will take the reader through the full design process of the augmented guitar. In addition, this paper also discusses more about MIDI protocols for guitar augmentation. Other than this, the steps involved in creating the dataset for mapping the guitar in 2dimensional space is also discussed in this research article.},
	language = {en},
	author = {John, Jonath},
	year = {2020},
	pages = {10},
}

@article{martelloni_percussive_2020,
	title = {Percussive {Fingerstyle} {Guitar} through the {Lens} of {NIME}: an {Interview} {Study}},
	abstract = {Percussive ﬁngerstyle is a playing technique adopted by many contemporary acoustic guitarists, and it has grown substantially in popularity over the last decade. Its foundations lie in the use of the guitar’s body for percussive lines, and in the extended range given by the novel use of altered tunings. There are very few formal accounts of percussive ﬁngerstyle, therefore, we devised an interview study to investigate its approach to composition, performance and musical experimentation. Our aim was to gain insight into the technique from a gesture-based point of view, observe whether modern ﬁngerstyle shares similarities to the approaches in NIME practice and investigate possible avenues for guitar augmentations inspired by the percussive technique. We conducted an inductive thematic analysis on the transcribed interviews: our ﬁndings highlight the participants’ material-based approach to musical interaction and we present a three-zone model of the most common percussive gestures on the guitar’s body. Furthermore, we examine current trends in Digital Musical Instruments, especially in guitar augmentation, and we discuss possible future directions in augmented guitars in light of the interviewees’ perspectives.},
	language = {en},
	author = {Martelloni, Andrea and McPherson, Andrew and Barthet, Mathieu},
	year = {2020},
	pages = {6},
}

@article{barthet_music_2011,
	title = {Music recommendation for music learning: {Hotttabs}, a multimedia guitar tutor},
	abstract = {Music recommendation systems built on top of music information retrieval (MIR) technologies are usually designed to provide new ways to discover and listen to digital music collections. However, they do not typically assist in another important aspect of musical activity, music learning. In this study we present the application Hotttabs, an online music recommendation system dedicated to guitar learning. Hotttabs makes use of The Echo Nest music platform to retrieve the latest popular or “hot” songs based on editorial, social and charts/sales criteria, and YouTube to ﬁnd relevant guitar video tutorials. The audio tracks of the YouTube videos are processed with an automatic chord extraction algorithm in order to provide a visual feedback of the chord labels synchronised with the video. Guitar tablatures, a form of music notation showing instrument ﬁngerings, are mined from the web and their chord sequences are extracted. The tablatures are then clustered based on the songs’ chord sequences complexity so that guitarists can pick up those adapted to their performance skills.},
	language = {en},
	author = {Barthet, Mathieu and Anglade, Amélie and Fazekas, Gyorgy and Kolozali, Sefki and Macrae, Robert},
	year = {2011},
	pages = {7},
}

